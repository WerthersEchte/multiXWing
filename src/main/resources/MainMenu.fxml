<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mmVBox" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.fh_kiel.roboticlab.multixwing.javafx.MainMenu">
   <children>
      <Button fx:id="mmNewGame" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newGame" text="New Game">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openOptions" text="Options">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="mmClose" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#close" text="Close">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
